            <string>details    = "Having fun";
state      = "";
smallImage = "";

switch room
{
    case init:
        details = "Loading...";
    break;

    case intro:
        details = "Admiring main menu";
        switch menumanager.menu_level
        {
            case menulevel.achievements: details = "Checking achievements"; break;
            case menulevel.styles: details = "Changing styles"; break;
            case menulevel.statsmenu: details = "Looking over stats"; break;
            case menulevel.levels: details = "Selecting a level"; break;
            case menulevel.modmenu: details = "Selecting gameplay modifiers"; break;
            case menulevel.songsmenu: details = "Browsing the songs"; break;
            
            case menulevel.exitgame:
            
                details = "Abandoning his goal";
                state = "(Exit menu)"
                smallImage = "sopainn4_discord_rtp";
                
                if stat_get("FINISHED_GAME")
                {
                    details = "Finally resting"
                    smallImage = "";
                }
                
            break;
        }
        
        if !intromanager.introFinished and !intromanager.pushFlicker
        {details = "Watching intro";}
        
        //discord_set_details(details);
    break;
    
    case level_rewardroom:
        details = "Celebrating victory";
        state = "(Level Complete)"
    break;
    
    case level_gameover:
        details = "Recovering from defeat";
        state = "(Game Over)"
    break;
    
    
    //OTHER//
    default:
        details = "Playing";
        state = "["+room_get_name(room)+"]";
    
}

if OLDdetails != details
{
    discord_set_image_small(smallImage);
    discord_set_details(details);
    discord_set_state(state);
    discord_presence_update();
    
    OLDdetails      = details;
    OLDstate        = state;
    OLDsmallImage   = smallImage;
    print("DISCORD STATUS UPDATED!");
}
