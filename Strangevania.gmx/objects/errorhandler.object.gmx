    var _fatal = false;
    if string_count("FATAL ERROR", _er) &gt;= 1 _fatal = true;
    
    if _fatal
    {
        window_set_cursor(cr_default);
        director.hideMouse = false;
        inputmanager.alarm[0] = inputmanager.mouseHideTime;
        
        show_message("FATAL ERROR! GAME WILL CLOSE!" + chr(13) + chr(10) + _er);
