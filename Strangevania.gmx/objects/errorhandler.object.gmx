        window_set_cursor(cr_default);
        director.hideMouse = false;
        inputmanager.alarm[0] = inputmanager.mouseHideTime;
        
        show_message("Minor Error: " + chr(13) + chr(10) + _er);
    }
    _er = catch_error_dequeue();
}
