        if alarm_get(i) &gt; 0 {alarm[i]++}
    }
    exit;
}

if itemDefaultGravity
{
    vsp = vsp + grv;
    if vsp &gt;= itemDefaultGravityMax {vsp = itemDefaultGravityMax;}
}

if itemDefaultVerticalCollision
{
    //Vertical Collision
    if place_meeting( x, y+vsp, obj_solid)
    {
        while !place_meeting(x, y+sign(vsp), obj_solid)
        {
            y = y + sign(vsp)
        }
        vsp = 0;
    }
    y = y + vsp;
}

if !landed and place_meeting( x, y+1, obj_solid)
{
    alarm[11] = decayTime*room_speed;
    landed = true;
}

if alarm[11] &lt; 1.5*room_speed and landed
{
    decay = true;
}
